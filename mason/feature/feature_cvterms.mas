<%args>
    $feature
</%args>

% my @fcvt = $feature->search_related('feature_cvterms', undef, { prefetch => { 'cvterm' => 'dbxref' } } );
% if( @fcvt ) {
<ul>

%   for my $fcvt ( @fcvt ) {
%     my $cvt    = $fcvt->cvterm;
%     my $dbxref = $cvt->dbxref;
      <li>
         <a href="<% $dbxref->db->urlprefix .  $dbxref->db->url .  $dbxref->accession %>"><% $dbxref->db->name %>:<% $dbxref->accession %></a>
         &ndash;
         <% cvterm_link( $fcvt->cvterm ) %>
      </li>
%   }

</ul>
% }


<%once>
  use SGN::View::Feature qw/ cvterm_link /;
</%once>
