  <input type="hidden" id="gg_selected_population_type" value="" />
</div>

<input type="hidden" id="training_traits_ids" value="<% $training_traits_ids %>" /> 
<input type="hidden" id="selection_traits_ids" value="" />
<input type="hidden" id="selection_pop_id" value="" />
