<style>


.bar rect {
  shape-rendering: crispEdges;
  margin: 50px;
  stroke: white;
}

.axis path, .axis line {
  fill: none;
  stroke: #000;
  shape-rendering: crispEdges;
}

</style>

<& /util/import_css.mas, paths => ['/static/css/d3-tip.css'] &>

 <& /util/import_javascript.mas, classes => ["d3.d3Min",  "solGS.histogram", "solGS.phenoHistogram"] &>

