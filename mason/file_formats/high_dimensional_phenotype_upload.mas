                    <tr>
                        <td>sample_name</td>
                        <td>device_id</td>
                        <td>comments</td>
                        <td>wavelength columns (e.g. 400, 1200, etc)</td>
                    </tr>
                </tbody>
            </table>

        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>


