
<%doc>

=head1 NAME

/breeders_toolbox/trial.mas - a mason component to display a trial detail page

=head1 AUTHOR

Jeremy D. Edwards

=cut

</%doc>

<%args>
$trial_id
$trial_name
$breeding_program_id
$breeding_program_name
$location_id
$location_name
$year => undef
$trial_type => undef
$trial_type_id => undef
$planting_date => undef
$harvest_date => undef
$trial_description => undef
$user_can_modify => undef
$has_plant_entries => undef
$has_subplot_entries => undef
$has_tissue_sample_entries => undef
$hidap_enabled => undef
$folder_name => undef
$folder_id => undef
$plants_per_plot => undef
$has_expression_atlas => undef
$expression_atlas_url => undef
$site_project_name => undef
$trial_phenotype_files => undef
$sgn_session_id => undef
$user_name => undef
$design_name => undef
$phenotypes_fully_uploaded => undef
$has_col_and_row_numbers => undef
$assayed_traits => undef
$identifier_prefix => 'SGN'
$main_production_site_url
</%args>


<& /util/import_javascript.mas, classes => [ 'jquery.iframe-post-form', 'CXGN.Trial' , 'jstree/dist/jstree', 'CXGN.BreedersToolbox.HTMLSelect', 'moment_min', 'daterangepicker' ] &>


<br/>

<& /page/page_title.mas, title=>"Trial detail for ".$trial_name &>

<&| /page/info_section.mas, title=>"Trial details",  collapsible => 1, subtitle => '<button class="btn btn-primary btn-sm" id="edit_trial_details" style="margin:3px">Edit Trial Details</button>'&>


    <div class="panel panel-default">
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-8">

                <table class="table table-hover table-bordered" >

                    <tr>
                        <td><b>Trial Name</b></td>
                        <td>
                            <div id="trial_name">
% if ($trial_name) {
%   print "$trial_name";
% } else {
%   print "<span class='text-danger'>[No Trial Name]</span>";
% }
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <td><b>Breeding Program </b></td>
                        <td>
                            <span id="breeding_program">
% if ($breeding_program_name) {
%  print "$breeding_program_name";
% }
% else {
%  print "<span class='text-danger'>[No Breeding Program]</span>";
% }
                            </span>
                        </td>
                    </tr>

                    <tr>
                        <td><b>Trial Location</b></td>
                        <td>
                            <div id="trial_location">
% if ($location_name) {
%  print "$location_name";
% } else {
%  print "<span class='text-danger'>[No Location]</span>";
% }
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <td><b>Year</b></td>
                        <td>
                            <div id="trial_year">
% if ($year) {
%  print "$year";
% } else {
%  print "<span class='text-danger'>[No Year]</span>";
% }
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <td><b>Trial Type</b></td>
                        <td>
                            <div id="trial_type">
% if ($trial_type) {
%  print "$trial_type";
% } else {
% print "<span class='text-danger'>[Type not set]</span>";
% }
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <td><b>Planting Date</b></td>
                        <td>
                            <div id="planting_date">
% if ($planting_date) {
  <a href="/breeders/trial/<% $trial_id %>?currentDate=<% $planting_date %>" ><% $planting_date %></a>
% } else {
%  print "<span class='text-danger'>[No Planting Date]</span>";
% }
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <td><b>Harvest Date</b></td>
                        <td>
                            <div id="harvest_date">
% if ($harvest_date) {
  <a href="/breeders/trial/<% $trial_id %>?currentDate=<% $harvest_date %>" ><% $harvest_date %></a>
% } else {
%  print "<span class='text-danger'>[No Harvest Date]</span>";
% }
                            </div>
                        </td>
                    </tr>

                    <tr><td><b>Description</b></td>
                        <td>
                            <div id="trial_description">
% if ($trial_description) {
%  print "$trial_description";
% } else {
%  print "<span class='text-danger'>[No Description]</span>";
% }
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <td><b>Folder</b></td>
                        <td>
                            <span id="trial_folder_div"><a href="/folder/<% $folder_id %>"><% $folder_name %></a></span>
                            <br/><br/>
                            <button class="btn btn-sm btn-default" style="margin:3px" id="new_folder_dialog_link">New Folder</button> <button class="btn btn-sm btn-default" style="margin:3px" id="open_folder_dialog_link">Change Folder</button>
                        </td>
                    </tr>

                </table>

            </div>
            <div class="col-sm-4">
                <& /util/barcode.mas, identifier => "$identifier_prefix"."$trial_id", trial_id=> "$trial_id", trial_name=> "$trial_name", format=>"trial_qrcode"  &>
            </div>
        </div>

    </div>
    </div><!-- closes info section -->

</&>

<& /breeders_toolbox/folder/folder_set.mas, breeding_program_id => $breeding_program_id, breeding_program_name =>$breeding_program_name, trial_id => $trial_id, trial_name =>$trial_name &>
<& /breeders_toolbox/folder/folder_new.mas, breeding_program_id => $breeding_program_id, breeding_program_name =>$breeding_program_name &>
<& /breeders_toolbox/trial/edit_trial_details_dialog.mas, trial_name => $trial_name, breeding_program => $breeding_program_id, location => $location_id, year => $year, trial_type => $trial_type, trial_type_id => $trial_type_id, planting_date => $planting_date, harvest_date => $harvest_date, trial_description => $trial_description &>

<&| /page/info_section.mas, title=>"Navigator",  collapsible => 1 &>
    <div class="row">
        <div class="col-sm-6">
            <div class="well">
                <div class="row">
                    <div class="col-sm-3">
                        <span style="font-size:2.5em;" class="glyphicon glyphicon-qrcode"></span>
                    </div>
                    <div class="col-sm-7">
                        <h4>Generate barcode labels for plots or plants or accessions in this trial.</h4>
                    </div>
                    <div class="col-sm-2">
                        <button id="generate_trial_barcode_link" class="btn btn-primary btn-sm">Go</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="well">
                <div class="row">
                    <div class="col-sm-4">
                        <span style="font-size:2.5em;" class="glyphicon glyphicon-pencil"></span>
                    </div>
                    <div class="col-sm-6">
                        <h4>Directly record phenotypes to database for this trial. </h4>
                    </div>
                    <div class="col-sm-2">
                        <button id="direct_phenotyping_link" class="btn btn-primary btn-sm">Go</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-th"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Field Layout Tools</h4>

% my $subtitle;
% if ($has_col_and_row_numbers){
%     $subtitle = '<button class="btn btn-sm btn-primary" style="margin:3px" id="heatmap_upload_trial_coords_link">Upload trial coordinates</button> <button class="btn btn-sm btn-default" style="margin:3px" id="edit_field_map_link">Edit Field Map</button> <button class="btn btn-sm btn-default" style="margin:3px" id="trial_fieldmap_download_layout_button">Download FieldMap Layout</button> <button class="btn btn-sm btn-default" style="margin:3px" data-toggle="modal" data-target="#fieldmap_usage_modal">Usage Help <span class="glyphicon glyphicon-question-sign"></span></button>';
% }else{
%    $subtitle = '<button class="btn btn-sm btn-primary" style="margin:3px" id="heatmap_upload_trial_coords_link">Upload trial coordinates</button> <button class="btn btn-sm btn-default" style="margin:3px" id="edit_field_map_link">Edit Field Map</button> <button class="btn btn-sm btn-default" style="margin:3px" data-toggle="modal" data-target="#fieldmap_usage_modal">Usage Help <span class="glyphicon glyphicon-question-sign"></span></button>';
% }

                        <&| /page/info_section.mas, id => "pheno_heatmap", title=>"Trial Heatmap", collapsible=>1, collapsed => 1, subtitle=> $subtitle &>
                            <& /breeders_toolbox/trial/phenotype_heatmap.mas, trial_id => $trial_id &>
                        </&>

                        <%doc><&| /page/info_section.mas, id => "physical_layout", title=>"Physical Trial Layout", collapsible=>1, collapsed => 1  &>
                            <& /breeders_toolbox/trial/trial_coords.mas, trial_id => $trial_id &>
                        </&></%doc>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-list-alt"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Trial Design Info</h4>

                        <&| /page/info_section.mas, id => "trial_design_section", title=>"Design", collapsible=>1, collapsed=>1, subtitle=>'<button class="btn btn-sm btn-primary" style="margin:3px" id="trial_download_layout_button">Download Layout</button>' &>

                            <div class="well well-sm">

                                <& /breeders_toolbox/trial/design.mas, trial_id => $trial_id &>

                                <&| /page/info_section.mas, id => "trial_accessions", title=>"Accessions", is_subsection => 1, collapsible=>1, collapsed=>1 &>
                                    <& /breeders_toolbox/trial/trial_accessions.mas, trial_id => $trial_id &>
                                </&>

                                <&| /page/info_section.mas, id => "trial_seedlots", title=>"Seedlots", is_subsection => 1, collapsible=>1, collapsed=>1, subtitle=>'<button class="btn btn-sm btn-default" style="margin:3px" id="trial_upload_seedlot_button" >Upload Seedlots Planted In Trial</button>' &>
                                    <& /breeders_toolbox/trial/trial_seedlots.mas, trial_id => $trial_id &>
                                </&>

                                <&| /page/info_section.mas, id => "trial_controls", title=>"Controls",  is_subsection => 1, collapsible=>1, collapsed=>1 &>
                                    <& /breeders_toolbox/trial/trial_controls.mas, trial_id => $trial_id &>
                                </&>

                                <&| /page/info_section.mas, id => "trial_plots", title=>"Plots",  is_subsection => 1, collapsible=>1, collapsed=>1, subtitle=>'<button class="btn btn-sm btn-default" style="margin:3px" id="trial_upload_plot_gps_button" >Upload GPS Coordinates</button>' &>
                                    <& /breeders_toolbox/trial/trial_plots.mas, trial_id => $trial_id &>
                                </&>

% if ($has_subplot_entries) {
                                <&| /page/info_section.mas, id => "trial_subplots", title=>"Subplots",  is_subsection => 1, collapsible=>1, collapsed=>1 &>
                                    <& /breeders_toolbox/trial/trial_subplots.mas, trial_id => $trial_id &>
                                </&>
% }

                                <&| /page/info_section.mas, id => "trial_plants", title=>"Plant Entries", is_subsection => 1, collapsible=> 1, collapsed=>1 &>

                                    <div id="plant_entries_section">

% if (!$has_plant_entries) {
                                       <button class="btn btn-default" id="create_plant_entries_button">Add plant entries</button>
                                       <button class="btn btn-default" id="upload_plant_entries_button">Upload plant entries</button>
% } else {
                                        <& /breeders_toolbox/trial/trial_plants.mas, trial_id => $trial_id &>
% }

                                    </div><!--closes plant_entries_section -->

                                </&>

                                <&| /page/info_section.mas, id => "trial_tissue_samples", title=>"Tissue Sample Entries", is_subsection => 1, collapsible=> 1, collapsed=>1 &>

                                    <div id="tissue_sample_entries_section">

% if (!$has_tissue_sample_entries) {
                                       <button class="btn btn-default" id="create_tissue_sample_entries_button">Add tissue sample entries</button>
                                       <& /breeders_toolbox/trial/add_tissue_sample_per_plant.mas, trial_id => $trial_id, trial_name => $trial_name, trial_has_plants => $has_plant_entries &>
% } else {
                                        <& /breeders_toolbox/trial/trial_tissue_samples.mas, trial_id => $trial_id &>
% }

                                    </div><!--closes tissue_sample_entries_section -->

                                </&>

                                <&| /page/info_section.mas, id => "trial_treatments", title=>"Field Management Factors",  is_subsection => 1, collapsible=>1, collapsed=>1, subtitle=>'<button class="btn btn-sm btn-default" style="margin:3px" id="trial_detail_page_add_treatment">Add Management Factor</button>' &>
                                    <& /breeders_toolbox/trial/trial_treatments.mas, trial_id => $trial_id &>
                                </&>

                            </div><!-- closes well -->

                        </&>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-equalizer"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Stored Phenotypes</h4>

% my $download_pheno_subtitle;
% if (scalar(@$assayed_traits) > 0){
%   $download_pheno_subtitle = "<button class='btn btn-sm btn-primary' style='margin:3px' id='trial_download_phenotypes_button'>Download Trial Data</button>"
% }
% else {
%   $download_pheno_subtitle = "Trial has no phenotype for download";
% }

                        <&| /page/info_section.mas, id=> "trial_detail_traits_assayed", title => "Traits assayed", collapsible=>1, collapsed=>1, hide_if_empty=>1, subtitle=> $download_pheno_subtitle &>

% if ($has_expression_atlas) {
                            <& /breeders_toolbox/trial/export_trial_to_expression_atlas.mas, trial_id => $trial_id, trial_name=>$trial_name, expression_atlas_url=>$expression_atlas_url, site_project_name=>$site_project_name, sgn_session_id=>$sgn_session_id, user_name=>$user_name, main_production_site_url=>$main_production_site_url &>
% }

                            <& /breeders_toolbox/trial/phenotype_summary.mas, trial_id => $trial_id &>

                        </&>

                        <&| /page/info_section.mas, id=> "compute_derived_traits", title=>'Compute Trait Phenotypes', collapsible=>1, collapsed=>1, &>
                            <& /breeders_toolbox/trial/derived_trait.mas, trial_id => $trial_id &>
                        </&>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-paperclip"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Data collection files and additional files</h4>

                        <&| /page/info_section.mas, title=>"Files", collapsible=>1, collapsed=>1 &>

                            <div class="well well-sm">

                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <&| /page/info_section.mas, title=>"Data Collection Files", is_subsection => 1, collapsible=>1, collapsed=>0, &>
                                            <&| /page/info_section.mas, title=>"Phenotyping Spreadsheets", empty_message=>'', is_subsection => 1, collapsible=>0, collapsed=>0, subtitle=>'<button class="btn btn-sm btn-default" style="margin:3px" name="create_spreadsheet_link">Create Spreadsheet</button>' &>
                                            </&>
                                            <&| /page/info_section.mas, title=>"Android Field Book Layout", empty_message=>'', is_subsection => 1, collapsible=>0, collapsed=>0, subtitle=>'<button class="btn btn-sm btn-default" style="margin:3px" name="create_fieldbook_link">Create Field Book</button>' &>
                                            </&>
                                            <&| /page/info_section.mas, title=>"Data Collector Spreadsheet", empty_message=>'', is_subsection => 1, collapsible=>0, collapsed=>0, subtitle=> '<button class="btn btn-sm btn-default" style="margin:3px" id="create_DataCollector_link">Create DataCollector Spreadsheet</button>' &>
                                            </&>
                                        </&><!-- closes create new phenotyping file section -->
                                    </div>
                                </div>
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <&| /page/info_section.mas, title=>"Upload Phenotyping Files", is_subsection => 1, collapsible=>1, collapsed=>0, &>
                                            <&| /page/info_section.mas, title=>"Phenotyping Spreadsheets", empty_message=>'', is_subsection => 1, collapsible=>0, collapsed=>0, subtitle=>'<button class="btn btn-sm btn-primary" style="margin:3px" id="upload_spreadsheet_phenotypes_link">Upload</button>'&>
                                            </&>
                                            <&| /page/info_section.mas, title=>"Android Field Book Exported", empty_message=>'', is_subsection => 1, collapsible=>0, collapsed=>0, subtitle=>'<button class="btn btn-sm btn-primary" style="margin:3px" id="upload_fieldbook_phenotypes_link">Upload</button>'&>
                                            </&>
                                            <&| /page/info_section.mas, title=>"Data Collector Spreadsheet", empty_message=>'', is_subsection => 1, collapsible=>0, collapsed=>0, subtitle=>'<button class="btn btn-sm btn-primary" style="margin:3px" id="upload_datacollector_phenotypes_link">Upload</button>'&>
                                            </&>
                                        </&><!-- closes upload new phenotype file section -->
                                    </div>
                                </div>
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <&| /page/info_section.mas, title=>"Uploaded Phenotyping Files", is_subsection => 1, collapsible=>1, collapsed=>0, subtitle=>'<button class="btn btn-sm btn-default" style="margin:3px" id="trial_set_phenotypes_fully_link">Phenotypes Fully Uploaded?</button>' &>

                                            <div style="overflow:scroll">
                                            <table class="table table-bordered files_datatables" alt="">
                                                <thead>
                                                    <tr><th>Filename</th><th>Date Uploaded</th><th>Uploaded By</th><th>File Type</th><th>Options</th></tr>
                                                </thead>
                                                <tbody>
% foreach my $f (@$trial_phenotype_files) {
                                                    <tr>
                                                      <td><% $f->[4] %></td>
                                                      <td><% $f->[1] %></td>
                                                      <td><a href="/solpeople/profile/<% $f->[2] %>"><% $f->[3] %></a></td>
                                                      <td><% $f->[6] %></td>
                                                      <td><a href="/breeders/phenotyping/view/<% $f->[0] %>">View</a> | <a href="/breeders/phenotyping/download/<% $f->[0] %>">Download</a></td>
                                                    </tr>
% }
                                                </tbody>
                                            </table>
                                            </div>

                                        </&><!-- closes uploaded file section -->
                                    </div>
                                </div>
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <&| /page/info_section.mas, title=>"Uploaded Additional Files", is_subsection => 1, collapsible=>1, collapsed=>0, subtitle=>'<button class="btn btn-sm btn-primary" style="margin:3px" id="trial_upload_additional_files_link">Upload Additional Files</button>' &>

                                        <& /breeders_toolbox/trial/upload_additional_file.mas, trial_id => $trial_id &>

                                        </&><!-- closes uploaded additional file section -->
                                    </div>
                                </div>

                            </div><!-- closes well -->

                        </&><!-- closes entire phenotyping file section -->

                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-signal"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Analysis Tools</h4>

                        <&| /page/info_section.mas, title=>"Analysis tools", collapsible=>1, collapsed=>1 &>
                            <!-- Phenotypic correlation analysis -->
                            <& /solgs/population/correlation.mas,
                                trial_id => $trial_id
                            &>

                            <& /anova.mas,
                                trial_id => $trial_id
                            &>

                            <!-- Population structure analysis -- PCA -->
                            <& /solgs/model/pca.mas,
                                trial_id => $trial_id
                            &>

% if ($hidap_enabled) {
                             <& /hidap/hidap_trial_link.mas, trial_id => $trial_id &>
% }

                        </&>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-cloud"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Weather data</h4>

                        <&| /page/info_section.mas, title => 'Weather at Trial Location', collapsible => 1, collapsed => 1 &>
                            <& /breeders_toolbox/trial/weather.mas, location => $location_name &>
                        </&>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-align-left"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>JBrowse Genome Alignments For this Trial</h4>

                        <&| /page/info_section.mas, title => 'Trial JBrowse', collapsible => 1, collapsed => 1 &>
                            <& /breeders_toolbox/trial/jbrowse_instance.mas, trial_id => $trial_id &>
                        </&>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-link"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Trial Linkage</h4>

                        <&| /page/info_section.mas, title => 'Field Trial to Field Trial Linkage', id => 'field_trial_from_field_trial_section', collapsible => 1, collapsed => 1 &>
                            <& /breeders_toolbox/trial/field_trial_from_field_trial_linkage.mas, trial_id => $trial_id &>
                        </&>
                        <&| /page/info_section.mas, title => 'Field Trial to Genotyping Trial Linkage', id => 'genotyping_trial_from_field_trial_section', collapsible => 1, collapsed => 1 &>
                            <& /breeders_toolbox/trial/genotyping_trial_from_field_trial_linkage.mas, trial_id => $trial_id, trial_type => 'field_trial' &>
                        </&>
                        <&| /page/info_section.mas, title => 'Field Trial to Crossing Trial Linkage', id => 'crossing_trial_from_field_trial_section', collapsible => 1, collapsed => 1 &>
                            <& /breeders_toolbox/trial/crossing_trial_from_field_trial_linkage.mas, trial_id => $trial_id, trial_type => 'field_trial' &>
                        </&>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="well">
                <div class="row">
                    <div class="col-sm-1">
                        <span style="font-size:3.5em;" class="glyphicon glyphicon-trash"></span>
                    </div>
                    <div class="col-sm-11">
                        <h4>Trial Deletion</h4>

                        <&| /page/info_section.mas, title=>'Delete trial data', subtitle=>'<font color="red" role="">Deletion cannot be undone</font>', collapsed=>1, collapsible=>1 &>
                          <& /breeders_toolbox/trial/trial_deletion.mas, trial_id => $trial_id  &>
                        </&>
