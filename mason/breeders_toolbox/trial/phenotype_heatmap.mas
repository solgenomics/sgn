    jQuery("#invert_row_checkmark").change(function() {
        FieldMap.meta_data.invert_row_checkmark = document.getElementById("invert_row_checkmark").checked;
        FieldMap.load();
    });
    jQuery('#submit_fieldmap').click( function() {
        new jQuery.ajax({
            type: "POST",
