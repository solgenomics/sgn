        }
        PosX = PosX - ImgPos[0];
        PosY = PosY - ImgPos[1];

        //ctx.clearRect(0,0,background_image_width,background_image_height);

        var hovering_plot_name;
        for (key in drone_imagery_plot_polygons_display) {
            if (drone_imagery_plot_polygons_display.hasOwnProperty(key)) {
