<%args>
$trial_id
$trial_name
</%args>

<div class="modal fade" id="upload_propagation_group_ids_dialog" name="upload_propagation_group_ids_dialog" tabindex="-1" role="dialog" aria-labelledby="uploadPropagationGroupIDsDialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="reset" class="close" id="upload_propagation_group_dismiss_button_2" name="upload_propagation_group_dismiss_button" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="uploadPropagationGroupIDsDialog">Upload Propagation Group Identifiers</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <&| /page/explanation.mas, title=>'Template Information' &>
                        <p>
                            <b>File format information</b>
                            <br>
                            <a id="propagation_group_ids_upload_spreadsheet_format_info">Spreadsheet Format</a>
                        </p>
                    </&>
                    <form method="post" enctype="multipart/form-data" encoding="multipart/form-data" id="upload_propagation_group_ids_form" name="upload_propagation_group_ids_form">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Upload File: </label>
                            <div class="col-sm-9" >
                                <input type="file" name="propagation_group_ids_file" id="propagation_group_ids_file" encoding="multipart/form-data" />
                                <input name="propagation_project_id" id="propagation_project_id" type="hidden" value="<% $trial_id %>">
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button id="upload_propagation_group_dismiss_button_1" name="upload_propagation_group_dismiss_button" type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" name="upload_propagation_group_ids_submit" id="upload_propagation_group_ids_submit">Upload</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="propagation_group_ids_saved_dialog_message" name="propagation_group_ids_saved_dialog_message" tabindex="-1" role="dialog" aria-labelledby="propagationGroupIdsSavedDialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="propagationGroupIdsSavedDialog">Success</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <p>
                        <span class="ui-icon ui-icon-circle-check" style="float: left; margin: 0 7px 50px 0;"></span>
                        The propagation group ids were saved successfully.
                    </p>
                </div>
            </div>
            <div class="modal-footer">
                <button id="dismiss_propagation_group_ids_saved_dialog" type="button" class="btn btn-default" data-dismiss="modal">Close & Reload</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="upload_propagation_group_ids_error_display" name="upload_propagation_group_ids_error_display" tabindex="-1" role="dialog" aria-labelledby="uploadPropagationGroupIdsErrorDialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="uploadPropagationGroupIdsErrorDialog">Upload Propagation Group Identifiers Error</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <table>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="propagation_group_id_upload_spreadsheet_info_dialog" name="propagation_group_id_upload_spreadsheet_info_dialog" tabindex="-1" role="dialog" aria-labelledby="uploadPropagationGroupIDInfoDialog">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header" style="text-align: center">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="uploadPropagationGroupIDInfoDialog">Upload Template Information For Propagation Group Identifiers</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <b>Header:</b>
                    <br>
                    The first row (header) should contain the following:
                    <br />

                    <table class="table table-hover table-bordered table-condensed" >
                        <thead></thead>
                        <tbody>
                            <tr>
                                <td>propagation_group_identifier</td>
                                <td>purpose</td>
                                <td>accession_name</td>
                                <td>material_type</td>
                                <td>source_type</td>
                                <td>source_name</td>
                                <td>date</td>
                                <td>sub_location</td>
                                <td>description</td>
                                <td>operator_name</td>
                            </tr>
                        </tbody>
                    </table>

                    <b>Required fields:</b>
                    <ul>
                        <li>propagation_group_identifier (must be globally unique and must not exist in the database )</li>
                        <li>purpose (e.g., CVC Backup, SH second Tree, SH Replacement)</li>
                        <li>accession_name (must exist as <b>uniquenames</b> in the database)</li>
                        <li>material_type (must be one of the following: plant, seed, budwood, tissue culture. If you would like to add additional types, please contact us.)</li>
                        <li>date (propagation initiation date YYYY-MM-DD)</li>
                        <li>description</li>
                        <li>operator_name</li>
                    </ul>

                    <b>Optional fields:</b>
                    <ul>
                        <li>source_type (can be plot, subplot, plant, tissue sample or new)</li>
                        <li>source_name (must exist as <b>uniquenames</b> in the database, can be plot, subplot, plant or tissue sample stock type.)</li>
                        <li>sub_location (detailed location of propagation materials)</li>
                    </ul>
                </div>
                <& /help/file_upload_type.mas, type => "Propagation Group Identifiers", optional_column => 1 &>
            </div>
            <div class="modal-footer">
                <button id="close_propagation_group_id_info_upload_dialog" type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<script>

jQuery(document).ready(function(){

    jQuery('#upload_propagation_group_ids_link').click(function() {
        jQuery("#upload_propagation_group_ids_dialog").modal("show");
    });

    jQuery('#upload_propagation_group_ids_spreadsheet_format_info').click(function(){
        jQuery("#propagation_group_ids_upload_spreadsheet_info_dialog").modal("show");
    });

    jQuery("#upload_propagation_group_ids_submit").click(function(){
        upload_propagation_group_ids();
    });

    function upload_propagation_group_ids() {
        var uploadFile = jQuery("#propagation_group_ids_file").val();
        if (uploadFile === '') {
            alert("Please select a file");
            return;
        }

        jQuery("#working_modal").modal("show");
        jQuery.ajax({
            url: "/ajax/propagation/upload_propagation_group_identifiers",
            type: 'POST',
            data: new FormData(jQuery("#upload_propagation_group_ids_form")[0]),
            processData: false,
            contentType: false,
            success: function(response) {
                jQuery("#working_modal").modal("hide");
                if (response.error_string) {
                    jQuery("#upload_propagation_group_ids_error_display tbody").html('');
                    jQuery("#upload_propagation_group_ids_error_display tbody").append(response.error_string);
                    jQuery("#upload_propagation_group_ids_error_display").modal("show");
                    return;
                }
                if (response.error) {
                    alert(response.error);
                    return;
                }
                if (response.success) {
                    jQuery("#upload_propagation_group_ids_dialog").modal("hide");
                    jQuery('#propagation_group_ids_saved_dialog_message').modal("show");
                }
            }
        });
    };

    jQuery("#dismiss_propagation_group_ids_saved_dialog").click(function(){
        location.reload();
    });

    jQuery('#propagation_group_ids_upload_spreadsheet_format_info').click(function() {
        jQuery("#propagation_group_id_upload_spreadsheet_info_dialog").modal("show");
    });

    jQuery("[name='upload_propagation_group_dismiss_button']").click(function() {
        jQuery('#upload_propagation_group_ids_dialog').modal('hide');
        jQuery('#propagation_group_ids_file').val('');
    });

});

</script>
